# Nombres premiers de Germain
def premiers_de_Germain_après(n):
    n += 2 if n % 2 else 1
    while True:
        if est_premier(n):
            if est_premier(2 * n + 1):
                return n,2 * n + 1
        n += 2
print(f"premiers de Germain après 100000 : {premiers_de_Germain_après(100000)}")
