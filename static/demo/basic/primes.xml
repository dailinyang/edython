# Nombres premiers jumeaux
def nombres_premiers_jumeaux_après(n):
    n += 2 if n % 2 else 1
    while True:
        if est_premier(n):
            if est_premier(n + 2):
                return n,n + 2
            else:
                n += 4
        else:
            n += 2
print(f"premiers jumeaux après 100000 : {nombres_premiers_jumeaux_après(100000)}")
