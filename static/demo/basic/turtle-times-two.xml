<python>
# cercle x 2
from turtle import *
from math import *
edython.turtleSetup()
try:
    n = int(input('Entrer un entier entre 1 et 240'))
    assert n &gt;= 1 and n &lt;= 240
except:
    print(f"{n} n'est pas un entier entre 1 et 200 !")
    return
r = 150
speed('fast')
x = lambda k: cos(2 * k * pi / n)* r
y = lambda k: sin(2 * k * pi / n)* r
def ligne(i,j):
    penup()
    goto(x(i),y(i))
    pendown()
    goto(x(j),y(j))
for k in range(n // 2):
    ligne(2 * k + 1,4 * k + 2)
    ligne(4 * k + 4,2 * k + 2)
done()
</python>
