  # Cliquez sur "marche" et soyez patient (20'')
  from turtle import *
  from math import *
  edython.turtleSetup()
  limit = 5
  pensize(1)
  fillcolor('red')
  left(90)
  def dessine_carré(côté):
      pendown()
      begin_fill()
      for i in range(4):
          forward(côté)
          left(90)
      end_fill()
      penup()
  def dessine_arbre(côté):
      if côté &gt; limit:
          dessine_carré(côté)
          forward(côté)
          right(45)
          dessine_arbre(côté / sqrt(2))
          left(90)
          forward(côté / sqrt(2))
          dessine_arbre(côté / sqrt(2))
          left(90)
          forward(côté / sqrt(2))
          left(45)
          forward(côté)
          left(90)
          forward(côté)
          left(90)
  dessine_arbre(50)
  done()
